{
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "es5",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.yaml,*.yml",
      "options": {
        "printWidth": 120,
        "singleQuote": false,
        "proseWrap": "preserve",
        "endOfLine": "lf",
        "yaml": {
          "documentStart": true,
          "documentEnd": true
        }
      }
    }
  ]
}
